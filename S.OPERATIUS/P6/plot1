#!/usr/bin/gnuplot
set auto x
set yrange [0:20]
set style data histogram
set style histogram cluster gap 1
set style fill transparent solid 0.75 border -3
set boxwidth 0.75
set bmargin 3
set grid x
set grid y
set xlabel "Agrupacions"
set ylabel "Vots (Percentatge)"
plot 'dades' u 2:xticlabels(1) w boxes ls 2 notitle
pause -1